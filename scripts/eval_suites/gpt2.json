{
  "defaults": {
    "backend": "faer",
    "seed": 0,
    "rtol": 0.0001,
    "atol": 0.0001,
    "warmup": 1,
    "iters": 3,
    "threads": [1, 4],
    "params": {
      "prompt": "Hello",
      "torch_model": "gpt2",
      "checkpoint": "checkpoints/gpt2.bin",
      "config": "configs/gpt2_model.json",
      "tokenizer": "configs/gpt2_tokenizer.json",
      "generate_tokens": 0,
      "bench_tokens": [1, 64],
      "kv_cache": true
    }
  },
  "runs": [
    {
      "model": "gpt2",
      "workload": ["validate", "bench"]
    }
  ]
}
